{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/(.*)", "destination": "/404.html", "missing": [
      { "type": "file", "value": "public/$1" },
      { "type": "file", "value": "out/$1" }
    ]}
  ]
}
