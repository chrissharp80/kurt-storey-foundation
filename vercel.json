{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "out",
  "trailingSlash": false,
  "cleanUrls": true,
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/why$", "dest": "/why.html" },
    { "src": "^/apply$", "dest": "/apply.html" },
    { "src": "^/catalog$", "dest": "/catalog.html" },
    { "handle": "filesystem" },
    { "src": "/_next/static/(.*)", "headers": { "cache-control": "public,max-age=31536000,immutable" } },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
}
