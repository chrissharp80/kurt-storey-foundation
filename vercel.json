{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "out",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/((?!_next|images).*)", "destination": "/404.html", "has": [{ "type": "header", "key": "x-vercel-error", "value": "404" }] }
  ]
}
